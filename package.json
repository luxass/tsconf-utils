{
  "name": "tsconf-utils",
  "type": "module",
  "version": "1.2.0",
  "description": "Utils for TSConfig files",
  "author": {
    "name": "Lucas Norgaard",
    "email": "lucasnrgaard@gmail.com",
    "url": "https://luxass.dev"
  },
  "license": "MIT",
  "homepage": "https://github.com/luxass/tsconf-utils",
  "repository": {
    "type": "git",
    "url": "https://github.com/luxass/tsconf-utils"
  },
  "bugs": {
    "url": "https://github.com/luxass/tsconf-utils/issues"
  },
  "keywords": [
    "tsconfig",
    "tsconfig.json",
    "config",
    "resolve"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=16"
  },
  "packageManager": "pnpm@8.6.0",
  "scripts": {
    "build": "tsup",
    "test": "vitest --run",
    "lint": "eslint --cache {src,tests}/**/*.ts"
  },
  "dependencies": {
    "jsonc-parse": "^1.0.2"
  },
  "devDependencies": {
    "@luxass/eslint-config": "^2.2.5",
    "@luxass/prettier-config": "^1.1.3",
    "@types/node": "^18.11.18",
    "eslint": "^8.31.0",
    "prettier": "^2.8.2",
    "tsup": "^6.5.0",
    "typescript": "^4.9.4",
    "vitest": "^0.31.0"
  }
}
