{
  "name": "tsconf-utils",
  "type": "module",
  "version": "1.2.0",
  "description": "Utils for TSConfig files",
  "author": {
    "name": "Lucas Norgaard",
    "email": "lucasnrgaard@gmail.com",
    "url": "https://luxass.dev"
  },
  "license": "MIT",
  "homepage": "https://github.com/luxass/tsconf-utils",
  "repository": {
    "type": "git",
    "url": "https://github.com/luxass/tsconf-utils"
  },
  "bugs": {
    "url": "https://github.com/luxass/tsconf-utils/issues"
  },
  "keywords": [
    "tsconfig",
    "tsconfig.json",
    "config",
    "resolve"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=16"
  },
  "packageManager": "pnpm@8.6.0",
  "scripts": {
    "build": "tsup",
    "test": "vitest --run",
    "lint": "eslint ."
  },
  "dependencies": {
    "jsonc-parse": "^1.0.3"
  },
  "devDependencies": {
    "@luxass/eslint-config": "^3.2.0",
    "@luxass/prettier-config": "^1.2.0",
    "@types/node": "^18.16.16",
    "eslint": "^8.41.0",
    "tsup": "^6.7.0",
    "typescript": "^5.0.4",
    "vitest": "^0.31.3"
  }
}
